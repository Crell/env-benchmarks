{
    "name": "crell/env-bench",
    "require": {
        "php": "~8.1",
        "crell/serde": "^0.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "phpbench/phpbench": "^1.1",
        "phpstan/phpstan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Crell\\EnvBench\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crell\\EnvBench\\": "tests",
            "Crell\\EnvBench\\Benchmarks\\": "benchmarks"
        }
    },
    "scripts": {
        "benchmarks": "vendor/bin/phpbench run benchmarks --report=aggregate"
    }
}
